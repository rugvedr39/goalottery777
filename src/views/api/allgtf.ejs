    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f7f8ff;
            margin: 0;
            padding: 0;
            overflow-x: hidden;
        }
        .app {
            width: 100%;
            max-width: 1200px;
            margin: 0 auto;
            padding: 10px;
            box-sizing: border-box;
        }
        .page_title {
            margin: 0;
            color: #fff;
            font-size: 20px;
        }
        main {
            display: flex;
            flex-wrap: wrap;
            padding: 20px 0;
            box-sizing: border-box;
            justify-content: center;
        }
        .jili_games_card {
            flex: 1 0 100%;
            max-width: 28%;
            height: 147px;
            margin: 10px 1%;
            background-color: #fff;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
            text-align: center;
            text-decoration: none;
            margin: 8px;
        }
        .jili_games_card img {
            width: 108px;
            height: 144px;
            cursor: pointer;
            object-fit: cover;
        }
    </style>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
