<script>
	document.addEventListener('DOMContentLoaded', function() {
		// Get all tab items
		var tabItems = document.querySelectorAll('.tabbar__container-item');
	  
		// Add click event listener to each tab item
		tabItems.forEach(function(item) {
		  item.addEventListener('click', function() {
			// Remove 'active' class from all tab items
			tabItems.forEach(function(tab) {
			  tab.classList.remove('active');
			});
	  
			// Add 'active' class to the clicked tab item
			item.classList.add('active');
		  });
		});
	  
		// Check if there's a hash in the URL and activate the corresponding tab
		var hash = window.location.hash;
		if (hash) {
		  var targetTab = document.querySelector('[onclick*="' + hash.substr(1) + '"]');
		  if (targetTab) {
			targetTab.classList.add('active'); // Add 'active' class to the target tab
		  }
		}
	  });
	  
  </script>
  
